##
# cd to P4 'home' for client space

nonegs() { 
	echo $1 | sed -e 's/-//g'
}

d="$1"
D=/Langley
A=unknown

if [ -z $d ]; then d=/Tools/Ant ; fi

if [ "$P4CLIENT" = "rmills-mpb13-7Xd2" ]; then A=X5d2 ; fi

if [ $A == unknown ]; then
	echo "*** P4CLIENT=$P4CLIENT unknown ***"
else
	cd ${D}/${A}/$(ls ${D}/${A})/$d
fi


# That's all Folks
##
