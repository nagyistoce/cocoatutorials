##
# cd to P4 'home' for client space

nonegs() { 
	echo $1 | sed -e 's/-//g'
}

d="$1"
if [ -z $d ]; then d=/Tools/Ant ; fi

A=
D=/Langley


if [ "$P4CLIENT" = "rmills-mpb13-7Xd2" ]; then A=X5d2/PESandbox ; fi
if [ "$P4CLIENT" = "rmills-mbp-X5d2"   ]; then A=X5d2/PESandbox ; fi

if [ ! -z $A ]; then
	cd ${D}/${A}/${d}
else
	echo "*** P4CLIENT=$P4CLIENT unknown ***"
fi

# That's all Folks
##
